<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:tint="#ADADAD">
    <item>
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45">
            <shape android:shape="line">
                <stroke android:width="2dp"/>
            </shape>
        </rotate>
    </item>
    <item>
        <rotate
            android:fromDegrees="-45"
            android:toDegrees="-45">
            <shape android:shape="line">
                <stroke android:width="2dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>